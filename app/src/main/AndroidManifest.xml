<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.itn.myprofile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConstraintLayout2Activity" android:label="@string/app_constraint"></activity>
        <activity
            android:name=".ConstraintLayoutActivity"
            android:label="@string/app_constraint" />
        <activity
            android:name=".LayoutGroup2Activity"
            android:label="@string/app_layout" />
        <activity
            android:name=".LayoutGroupActivity"
            android:label="@string/app_layout" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>